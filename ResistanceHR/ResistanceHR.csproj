    <PreBuildEvent>REM    This script copies mod files currently used by the game into your repository's Libraries folder. This means you only need to update dependencies in one place rather than having to do it for each mod you work on.
ECHO "======= PRE-BUILD"

ECHO "    --- Defining directories"
SET dirBepinex="C:\Program Files (x86)\Steam\steamapps\common\Streets of Rogue\BepInEx\"
SET dirCore="%25dirBepinex%25core\"
SET dirPatchers="%25dirBepinex%25patchers\"
SET dirPlugins="%25dirBepinex%25plugins\"
CD /D "..\..\..\Libraries"
SET dirLibraries=%25cd%25\

ECHO "    --- Defining series"
SET importsCore=BTHarmonyUtils.dll
SET importsPatchers=RogueLibsPatcher.dll
SET importsPlugins=RogueLibsCore.dll

ECHO "    --- Copying files"
FOR %25%25a IN (%25importsCore%25) DO (XCOPY /s /y "%25dirCore%25%25%25~a" %25dirLibraries%25)
FOR %25%25a IN (%25importsPatchers%25) DO (XCOPY /s /y "%25dirPatchers%25%25%25~a" %25dirLibraries%25)
FOR %25%25a IN (%25importsPlugins%25) DO (XCOPY /s /y "%25dirPlugins%25%25%25~a" %25dirLibraries%25)</PreBuildEvent>
  </PropertyGroup>
